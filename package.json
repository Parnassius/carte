{
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "prettier": "3.8.1",
    "prettier-plugin-jinja-template": "2.1.0"
  },
  "prettier": {
    "plugins": ["prettier-plugin-jinja-template"],
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
      {
        "files": ["*.html"],
        "options": {
          "parser": "jinja-template"
        }
      }
    ]
  }
}
